#include<stdio.h>
int main(int argc, char const *argv[])
{
    int marks[3]; //declear array

    printf("Enter physics : ");
    scanf("%d",&marks[0]); // 0 means position of array we want to allocate

     printf("Enter chemistry : ");
    scanf("%d",&marks[1]);

     printf("Enter maths : ");
    scanf("%d",&marks[2]);

printf("physics =%d,chemister=%d,maths=%d",marks[0],marks[1],marks[2]);
    return 0;
}
