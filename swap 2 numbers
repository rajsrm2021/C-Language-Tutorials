#include<stdio.h>
int main(int argc, char const *argv[])
{
    int a , b;
    int t;
    
    printf("Enter 1st number 'a': ");
    scanf("%d",&a);
    
    printf("Enter 2nd number 'b': ");
    scanf("%d",&b);

    t=a;
    a=b;
    b=t;
    
    printf("number after swaping 'a' = %d\n",a);
    printf("number after swaping 'b' = %d\n",b);
    return 0;
}
